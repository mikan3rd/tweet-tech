.mypage
  %h2= @user.nickname
  %h3= "@" + @user.username

  = "いいね数：" + @user.likes.count.to_s
  %br

  - if @user.id == current_user.id
    = link_to 'ログアウト', destroy_user_session_path, method: :delete, class: "btn btn-default"
  %br
  %br
= render partial: 'tweets/tweets', collection: @tweets, as: :tweet
